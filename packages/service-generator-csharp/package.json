{
  "name": "@typespec/service-generator-csharp",
  "version": "0.48.0",
  "description": "TypeSpec service code generator for c-sharp",
  "homepage": "https://microsoft.github.io/typespec",
  "readme": "https://github.com/microsoft/typespec/blob/main/packages/service-generator-csharp/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsoft/typespec.git"
  },
  "bugs": {
    "url": "https://github.com/microsoft/typespec/issues"
  },
  "keywords": [
    "typespec",
    "service",
    "rejuvenate",
    "r9",
    "c-sharp"
  ],
  "author": "Microsoft Corporation",
  "license": "MIT",
  "type": "module",
  "main": "dist/src/index.js",
  "tspMain": "dist/src/index.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "clean": "rimraf ./dist ./temp",
    "build": "npm run compile",
    "compile": "tsc -p .",
    "watch": "tsc -p . --watch",
    "test": "mocha",
    "test-official": "c8 mocha --forbid-only --reporter mocha-multi-reporters",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "lint:fix": "eslint . --fix --ext .ts",
    "regen-docs": "tspd doc .  --enable-experimental  --output-dir ../../docs/emitters/service-generator-csharp/reference"
  },
  "files": [
    "lib/*.tsp",
    "dist/**",
    "!dist/test/**"
  ],
  "peerDependencies": {
    "@typespec/compiler": "workspace:~0.48.1",
    "@typespec/rest": "workspace:~0.48.0",
    "@typespec/http": "workspace:~0.48.0",
    "@typespec/versioning": "workspace:~0.48.0"
  },
  "dependencies": {
    "change-case": "~4.1.2"
  },
  "devDependencies": {
    "@typespec/compiler": "workspace:~0.48.1",
    "@typespec/openapi": "workspace:~0.48.0",
    "@typespec/rest": "workspace:~0.48.0",
    "@typespec/http": "workspace:~0.48.0",
    "@types/mocha": "~10.0.1",
    "@types/node": "~18.11.9",
    "@typespec/eslint-config-typespec": "workspace:~0.48.0",
    "@typespec/library-linter": "workspace:~0.48.0",
    "@typespec/eslint-plugin": "workspace:~0.48.0",
    "@typespec/tspd": "workspace:~0.46.0",
    "eslint": "^8.42.0",
    "c8": "~8.0.0",
    "rimraf": "~5.0.1",
    "typescript": "~5.1.3",
    "mocha": "~10.2.0",
    "mocha-junit-reporter": "~2.2.0",
    "mocha-multi-reporters": "~1.5.1"
  }
}
